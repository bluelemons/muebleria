<%= form_for @client do |f| %>

  <%= f.label :dni %><br>
  <%= f.text_field :dni %><br>

  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :adress %><br>
  <%= f.text_field :adress %><br>

  <%= f.label :email %><br>
  <%= f.text_field :email %><br>

  <%= f.label :phone %><br>
  <%= f.text_field :phone %><br>

  <%= f.label :cellphone %><br>
  <%= f.text_field :cellphone %><br>

  <%= f.submit "Guardar" %>

<% end %>
